{
  "version": 2,
  "functions": {
    "server/**/*.ts": { "runtime": "@vercel/node@2.0.0" }
  },
  "routes": [
    { "src": "/api/chat", "dest": "/server/api/chat.ts" },
    { "src": "/api/faq/categories", "dest": "/server/api/faq/categories.ts" },
    { "src": "/api/faq/select", "dest": "/server/api/faq/select.ts" },
    { "src": "/api/kb/stats", "dest": "/server/api/kb/stats.ts" },
    { "src": "/(.)", "dest": "/client/dist/$1" }
  ]
}
